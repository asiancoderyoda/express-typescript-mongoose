{
  "name": "express-typescript-mongoose",
  "version": "1.0.0",
  "description": "Express MongoDb Typescript REST boilerplate, mongoose as a ODM, inversify-js as a dependency container",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc",
    "watch": "tsc -w",
    "start": "node dist/server.js",
    "dev": "nodemon dist/server.js",
    "start2": "ts-node src/server.ts",
    "dev2": "nodemon --exec ts-node src/server.ts",
    "check-types": "tsc --pretty --noEmit",
    "check-format": "prettier --check .",
    "check-lint": "eslint . --ext ts --ext js",
    "format": "prettier --write .",
    "test-all": "npm run check-format && npm run check-lint && npm run check-types && npm run build",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/asiancoderyoda/express-typescript-mongoose.git"
  },
  "author": "Avigyan Bhakta",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/asiancoderyoda/express-typescript-mongoose/issues"
  },
  "homepage": "https://github.com/asiancoderyoda/express-typescript-mongoose#readme",
  "devDependencies": {
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/express-rate-limit": "^5.1.3",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/morgan": "^1.9.3",
    "@types/node": "^16.6.1",
    "cross-env": "^7.0.3",
    "eslint": "7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.0",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.12",
    "prettier": "^2.4.1",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.3"
  },
  "dependencies": {
    "apollo-server-express": "^3.3.0",
    "argon2": "^0.28.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-rate-limit": "^5.3.0",
    "express-validator": "^6.12.1",
    "helmet": "^4.6.0",
    "inversify": "^6.0.1",
    "jsonwebtoken": "^8.5.1",
    "log4js": "^6.3.0",
    "mongoose": "^6.2.2"
  }
}
